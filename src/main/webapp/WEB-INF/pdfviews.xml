<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="pdfIncomesView" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
		<property name="url" value="/WEB-INF/reports/incomesPdf.jrxml"/>
	</bean>
	
    <bean id="pdfOutcomesView" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
		<property name="url" value="/WEB-INF/reports/outcomesPdf.jrxml"/>
	</bean>

    <bean id="pdfPaymentsView" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
		<property name="url" value="/WEB-INF/reports/paymentsPdf.jrxml"/>
	</bean>
</beans>
